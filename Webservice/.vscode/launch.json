    "version": "0.2.0",
    "compounds": [
        {
            "name": "Launch & Debug Backend and Frontend",
            "configurations": ["START Backend", "START Frontend Chrome"] 
        }
    ],
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "START Frontend Chrome",
            "url": "http://localhost:8080/",
            "webRoot": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "START Backend",
            "program": "${workspaceFolder}\\ELIKADMain.js"
        }
    ]
}